{
    "AnimeMatch": {
        "ProcessNames": [
            "chrome",
            "msedge"
        ],
        "AnimeWebsites": [
            {
                "Website": "anime",
                "MatchText": [ "anime" ],
                "MatchAnimeNameStartText": [ "Watch" ],
                "MatchAnimeNameEndText": [ "Episode" ],
                "MatchEpisodeStartText": [ "Episode" ],
                "MatchEpisodeEndText": [ "English" ]
            }
        ]
    },
    "Program": {
        "ScanInterval": 3000,
        "UpdateIgnoreAll": false
    }
}